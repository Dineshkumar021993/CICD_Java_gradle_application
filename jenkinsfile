pipeline{
    agent any
    stages{
        stage('sonarquality check'){
            agent{
                docker{
                    image "openjdk:11"
                }
            }
            steps{
                script{
                    withSonarQubeEnv(credentialsId: 'sonar_token') {
                             sh "chmod 755 gradlew"
                             sh "./gradlew sonarqube"
    
                    }
                    
                }
            }
            
        }
    }
}
